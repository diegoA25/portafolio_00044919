#include <iostream>
#include <stack>

using namespace std;

//Se utilizo la octava actividad en donde la pila se tiene que dejar vacia.
int main (){
    stack<int> s;
    
    s.push(3);
    s.push(9);
    s.push(12);
    s.push(15);
    
    cout<<"El ultimo valor ingresado en la pila es: "<<s.top()<<endl;
    
    if(!s.empty()){
        s.pop();
        cout<<s.top()<<endl;
        s.pop();
        cout<<s.top()<<endl;
        s.pop();
        cout<<"El Ultimo valor de la pila es: "<<s.top()<<endl;
        s.pop();
        if(s.empty()){
            cout<<"La pila esta vacia"<<endl;
        }
    }
}
